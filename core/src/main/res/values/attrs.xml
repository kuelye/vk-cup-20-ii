<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BottomSheetLayout_Layout">
        <attr name="layout_expanded" format="boolean" />
    </declare-styleable>
    <declare-styleable name="Toolbar">
        <attr name="title" format="string" />
        <attr name="subtitle" format="string" />
    </declare-styleable>
    <declare-styleable name="BorderImageView">
        <attr name="borderColor" format="color" />
        <attr name="borderWidth" format="dimension" />
        <attr name="borderType" format="enum">
            <enum name="circle" value="0"/>
            <enum name="square" value="1"/>
        </attr>
        <attr name="borderCornerRadius" format="dimension" />
    </declare-styleable>
</resources>